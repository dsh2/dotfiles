load alias
alias -c *

alias Q exit -y
alias V set VERBOSE true
alias b banner
alias h help
alias j jobs -l -v
alias r resource
alias rr resource /home/dsf/.dotfiles/msfc.rc
alias sa show advanced
alias se sess
alias sl sessions -l -x
alias so show options
alias sr search
alias t threads -l -x
alias u use

setg ConsoleLogging true
setg LogLevel 3
setg PROMPT "%bld[%clr%grnmsf%clr: j:%J s:%S%bld]"
setg SessionLogging true
setg TimestampOutput true
